{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "functions": {
    "server.js": {
      "includeFiles": "data/final_dataset.csv", 
      "maxDuration": 60, 
      "memory": 3008
    }
  }
}